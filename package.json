{
    "name": "@yarle/ngx-smart-popover-demo",
    "version": "1.6.10",
    "resolutions": {
        "normalize-url": "6.1.0",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1"
    },
    "scripts": {
        "preinstall": "npx npm-force-resolutions",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:all": "ng build && npm run build:lib",
        "build:lib": "ng build ngx-smart-popover && npm run build:lib:scss && npm run build:lib:css",
        "build:lib:css": "sass ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/css/popover-content.component.css",
        "build:lib:scss": "mkdir ./dist/ngx-smart-popover/scss/ && cp ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/scss/popover-content.component.scss",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e:entrypoint": "npm install && ng e2e",
        "e2e:docker": "docker build -t ngx-smart-popover-e2e-tests . && docker run -v /code/ngx-smart-popover:/usr/src/app --rm ngx-smart-popover-e2e-tests",
        "run": "ng-packagr -p package.json",
        "publish": "cp README.md dist/ngx-smart-popover/README.md && cd dist/ngx-smart-popover && npm publish"
    },
    "dependencies": {
        "@angular/animations": "14.1.1",
        "@angular/common": "14.1.1",
        "@angular/compiler": "14.1.1",
        "@angular/core": " 14.1.1",
        "@angular/forms": "14.1.1",
        "@angular/platform-browser": "14.1.1",
        "@angular/platform-browser-dynamic": "14.1.1",
        "@angular/router": "14.1.1",
        "bootstrap": "5.1.3",
        "core-js": "3.26.0",
        "rxjs": "7.5.6",
        "zone.js": "0.11.6"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.1.1",
        "@angular/cli": "14.1.1",
        "@angular/compiler-cli": "14.1.1",
        "@angular/language-service": "14.1.1",
        "@types/jasmine": "4.0.3",
        "@types/jasminewd2": "2.0.10",
        "@types/node": "18.11.9",
        "codelyzer": "6.0.2",
        "jasmine-core": "4.2.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.0.0",
        "ng-packagr": "12.2.7",
        "normalize-url": "6.1.0",
        "protractor": "7.0.0",
        "protractor-jasmine2-screenshot-reporter": "0.5.0",
        "sass": "1.53.0",
        "scss-bundle": "3.1.2",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1",
        "ts-node": "10.9.1",
        "tsickle": "0.40.0",
        "tslib": "2.4.0",
        "tslint": "6.1.3",
        "typescript": "4.7.4"
    }
}
